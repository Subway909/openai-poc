{
  "name": "openai-poc",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "nodemon -L -e ts --exec \"npm run build && npm start\" ",
    "build": "tsc -p ./tsconfig.json && npm copy-files",
    "debug": "npm run copy-files && tsc-watch --onSuccess \"node --inspect=0.0.0.0:9229 dist/src/server.js\" --onFailure \"echo WHOOPS! Server compilation failed\"",
    "copy-files": "cp -r -f ./src/public/ ./dist/",
    "db-test": "npx ts-node --esm ./src/dbtest.ts",
    "migrate": "node-pg-migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "langchain": "^0.0.68",
    "multer": "^1.4.5-lts.1",
    "node-pg-migrate": "^6.2.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.10.0",
    "tsc-watch": "^6.0.0"
  }
